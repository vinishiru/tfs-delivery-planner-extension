{
    "manifestVersion": 1,
    "id": "simply-delivery-planner",
    "publisher": "simply-devops",
    "version": "1.0.39",
    "name": "Delivery Planner",
    "description": "Azure DevOps Extension",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "Simply.DeliveryPlanner",
            "type": "ms.vss-web.hub",
            "description": "Planejador de entregadas da Simply.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Delivery Planner",
                "uri": "dist/index.html",
                "icon": "asset://static/sample-icon.png",
                "supportsMobile": true
            }
        }
    ],
    "scopes": [
        "vso.work"
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [        
        {
            "path": "dist",
            "addressable": true
        }
    ]
}