{
    "manifestVersion": 1,
    "id": "tfs-delivery-planner-extension",
    "version": "0.1.0",
    "name": "Simply Delivery Planner",
    "description": "Um hub para realizar um planejamento de PBIs entregáveis.",
    "publisher": "simply-devops",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
            }
        ],
    "icons": {
        "default": "images/logo.png"
     },
    "contributions": [
        {
            "id": "Simply.DeliveryPlanner",
            "type": "ms.vss-web.hub",
            "description": "Um hub para realizar um planejamento de PBIs entregáveis.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
                ],
            "properties": {
                "name": "Delivery Planner",
                "order": 99,
                "uri": "hello-world.html"
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.code"
    ],
    "categories": [
        "Plan and track"
    ],
    "files": [
        {
            "path": "hello-world.html", "addressable": true
        },
        {
            "path": "images/logo.png", "addressable": true
        }
    ]
}